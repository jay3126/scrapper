<p id="notice"><%= notice %></p>

<h1>Listing Scrappers</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scrappers.each do |scrapper| %>
      <tr>
        <td><%= scrapper.url %></td>
        <td><%= link_to 'Show', scrapper %></td>
        <td><%= link_to 'Edit', edit_scrapper_path(scrapper) %></td>
        <td><%= link_to 'Destroy', scrapper, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
  <tbody>
   <% @docs.each do |item| %>
   <tr>
   <% price=item.css(".s-price").text%>
   <h1><%= price.text %></h1>
   </tr>
   <%end%>
  </tbody>
</table>

<br>

<%= link_to 'New Scrapper', new_scrapper_path %>
